{
    "name": "bannerworld/corcel-woocommerce",
    "description": "WooCommerce plugin for Corcel",
    "homepage": "http://github.com/corcel/woocommerce",
    "license": "MIT",
    "authors": [
        {
            "name": "Krzysztof Grabania",
            "email": "krzysztof@grabania.pl",
            "homepage": "https://grabania.pl"
        }
    ],
    "require": {
        "php": "^8.2",
        "bannerworld/corcel": "^1.0.x-dev",
        "nesbot/carbon": "^2.66|^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^9.5.10|^10.5",
        "larastan/larastan": "^2.4",
        "laravel/pint": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Corcel\\WooCommerce\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Database\\Factories\\": "tests/database/factories/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/RobJonesWeb/bannerworld-corcel"
        }
    ]
}
